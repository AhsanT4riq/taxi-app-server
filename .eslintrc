{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "no-use-before-define": [2, "nofunc"],
    "comma-dangle": [0],
    "new-cap": 0,
    "max-len": [2, 15800000],
    "no-else-return": 0,
    "no-param-reassign": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "prefer-rest-params": 0
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": ["eslint:recommended", "airbnb/base"]
}
